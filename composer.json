{
    "name": "deolive/sngbecomm-php",
    "description": "A simple SNGB internet acquiring php library.",
    "keywords": [
        "sngb",
        "sngb ecommerce",
        "payment processing",
        "api"
    ],
    "homepage": "http://ecomm.sngb.ru/",
    "authors": [
        {
            "name": "Yura Zatsepin",
            "email": "yu.zatsepin@sngb.ru"
        },
        {
            "name": "Igor Bodnar",
            "email": "ibodnar@amaxlab.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-0": {
            "SNGBE": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "SNGBE\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
